<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<script src="includes/3rd/prototype/prototype.js" type="text/javascript"></script> 
<script src="includes/scripts/loader.js?load=event" type="text/javascript"></script>
<title>NTA Top</title>
<style type="text/css">
	body{
		font-size:12px;
	}
	#menu_1{background-image:url(includes/images/menu_over_1.jpg);}
	#menu_2{background-image:url(includes/images/menu_2.jpg);}
	#menu_3{background-image:url(includes/images/menu_3.jpg);}
	#menu_4{background-image:url(includes/images/menu_4.jpg);}
	#menu_5{background-image:url(includes/images/menu_5.jpg);}
	#menu_6{background-image:url(includes/images/menu_6.jpg);}
	#menu_7{background-image:url(includes/images/menu_7.jpg);}
	
	.menu_selected {
		width			:95px;
		text-align		:center;
		height			:22px;
		padding-top		:5px;
		float			:left;
		font-size		:12px;
		color			:#000000;
		cursor			:pointer;
	}
	.menu_unselected {
		width			:95px;
		text-align		:center;
		height			:22px;
		padding-top		:5px;
		float			:left;
		font-size		:12px;
		color			:#FFFFFF;
		cursor			:pointer;
	}
	#top_banner{
		height			:42px;
		width			:100%;
		float			:left;
		margin			:0px;
		padding-right	:0px;
		background-image:url(includes/images/page_02.jpg);
		background-repeat:repeat-x;
	}
	#topMenu{
		width			:100%;
		height			:22px;
		margin			:0px;
		padding			:0px;
		background-image:url(includes/images/menu_12.jpg);
		background-repeat:repeat-x;
		float			:left;
	}
	#banner_right{
		width   :216px!important;
		width   :227px;
		height   :18px;
		background:url(includes/images/top_bg.jpg) left no-repeat;
		font-size  :12px;
		color   :#16355e;
		padding:3px 0 0 11px;
		position:absolute;
		right:8px;
	}
	#banner_right img{padding:0 0 2px 0;}
	#banner_right a:link{
		font-size: 12px;
		color: #16355e;
		text-decoration:none;
	}
	#banner_right a:visited {
		font-size: 12px;
		color: #16355e;
		text-decoration:none;
	}
	#banner_right a:hover {
		font-size: 12px;
		text-decoration: none;
		color:#FF6600;
	}

	#banner_right .margin_left {
		margin-left:2ex;
	}
	#banner_right img{padding:0 0 2px 0;margin-right:1ex;}
</style>
<script language="javascript" type="text/javascript">
	var G_currentMenu = 1;
	function initMenu (id, loc) {
		if (id != G_currentMenu){
			$("menu_" + G_currentMenu).className = "menu_unselected";
			$("menu_" + G_currentMenu).style.background = "url(" + "includes/images/menu_" + G_currentMenu + ".jpg)";
			$("menu_" + id).className = "menu_selected";
			$("menu_" + id).style.background = "url(" + "includes/images/menu_over_" + G_currentMenu + ".jpg)";
			G_currentMenu = id;
			window.parent.switchLoc(loc);
		}
	}
	
	function showAccount() {
		Collapsar.publish("/account/show", {});
	}
	
	function showAbout() {
		Collapsar.publish("/help/showabout", {});
	}
</script>
</head>
<body style="margin:0px;overflow-x:hidden;overflow-y:hidden;">
<div id="top_banner">
	<div style="float:left;padding:0px;margin:0px;"><img src="includes/images/page_01.jpg" alt=""/></div>
<div id="banner_right">
<img src="includes/images/top_arrow.gif" align="middle" alt="帐号管理"/><a href="javascript:showAccount()">帐号管理</a>
<img src="includes/images/top_arrow.gif" align="middle" alt="绿盟科技" class="margin_left"/><a href="http://www.nsfocus.com" target="_blank">绿盟科技</a>
<img src="includes/images/top_arrow.gif" align="middle" alt="关于NTA" class="margin_left"/><a href="javascript:showAbout();">关于NTA</a>
</div>
</div>
<div id="topMenu">
	<div style="float:left"><img src="includes/images/menu_04.jpg" alt="menu"/></div>
	<div id="menu_1" class="menu_selected" onclick="initMenu(1, 'event')">告警监控</div>
	<div id="menu_2" class="menu_unselected" onclick="initMenu(2, 'device')">设备监控</div>
	<div id="menu_3" class="menu_unselected" onclick="initMenu(3, 'traffic')">流量分析</div>
	<div id="menu_4" class="menu_unselected" onclick="initMenu(4, 'route')">路由分析</div>
	<div id="menu_5" class="menu_unselected" onclick="initMenu(5, 'report')">统计报表</div>
	<div id="menu_6" class="menu_unselected" onclick="initMenu(6, 'log')">日志管理</div>	
	<div id="menu_7" class="menu_unselected" onclick="initMenu(7, 'runtime')">运行配置</div>
	<div id="menu_8" class="menu_unselected" onclick="initMenu(8, 'system')">系统管理</div>
	<div id="cancel" style="float:right"><img src="includes/images/cancel.jpg" style="cursor:hand" onclick="top.document.location='auth/logout.action'" alt="注销"/></div>
	<div style="float:left"><img src="includes/images/menu_11.jpg" alt="menu"/></div>
</div>
</body>


</html>