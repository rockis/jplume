<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
<script src="includes/3rd/prototype/prototype.js" type="text/javascript"></script> 
<script src="includes/scripts/loader.js?load=event" type="text/javascript"></script>
<title>NTA Top</title>
<style type="text/css">
	body{
		font-size:12px;
		font-family:  Arial, Helvetica, sans-serif, 宋体;
	}
	#menu_1{background-image:url(includes/images/menu_over_1.jpg);}
	#menu_2{background-image:url(includes/images/menu_2.jpg);}
	#menu_3{background-image:url(includes/images/menu_3.jpg);}
	#menu_4{background-image:url(includes/images/menu_4.jpg);}
	#menu_5{background-image:url(includes/images/menu_5.jpg);}
	#menu_6{background-image:url(includes/images/menu_6.jpg);}
	#menu_7{background-image:url(includes/images/menu_7.jpg);}
	#menu_8{background-image:url(includes/images/menu_8.jpg);}
	.menu_selected {
		width			:95px;
		text-align		:center;
		height			:22px;
		padding-top		:5px;
		float			:left;
		font-size		:12px;
		color			:#000000;
		cursor			:pointer;
	}
	.menu_unselected {
		width			:95px;
		text-align		:center;
		height			:22px;
		padding-top		:5px;
		float			:left;
		font-size		:12px;
		color			:#FFFFFF;
		cursor			:pointer;
	}
	#top_banner{
		height			:42px;
		width			:100%;
		float			:left;
		margin			:0px;
		padding-right	:0px;
		background-image:url(includes/images/page_02.jpg);
		background-repeat:repeat-x;
	}
	#topMenu{
		width			:100%;
		height			:22px;
		margin			:0px;
		padding			:0px;
		background-image:url(includes/images/menu_12.jpg);
		background-repeat:repeat-x;
		float			:left;
	}
	#banner_right{
		width			:216px!important;
		width			:216px;
		height			:18px;
		background-position:left 1px !important;
		background:url(includes/images/top_bg.jpg) white left 2px no-repeat;
		font-size		:12px;
		color			:#16355e;
		position:absolute;
		right:8px;
	}
	#banner_right div {
		background-position:right 1px!important;
		background:url(includes/images/top_bg.jpg) right 2px no-repeat;
		padding:2px 0 0 11px!important;
		padding:3px 0 0 11px;
		border:0px solid red;
	}
	#banner_right .margin_left {
		margin-left:2ex;
	}
	#banner_right img{padding:0 0 2px 0;margin-right:1ex;}
	#banner_right a:link{
		font-size: 12px;
		color: #16355e;
		text-decoration:none;
	}
	#banner_right a:visited {
		font-size: 12px;
		color: #16355e;
		text-decoration:none;
	}
	#banner_right a:hover {
		font-size: 12px;
		text-decoration: none;
		color:#FF6600;
	}
	#banner_right img{padding:0 0 2px 0;margin-right:1ex;}
</style>
<script language="javascript" type="text/javascript">
	var G_currentMenu = 1;
	function initMenu (id, loc) {
		if (id != G_currentMenu){
			$("menu_" + G_currentMenu).className = "menu_unselected";
			$("menu_" + G_currentMenu).style.background = "url(" + "includes/images/menu_" + G_currentMenu + ".jpg)";
			$("menu_" + id).className = "menu_selected";
			$("menu_" + id).style.background = "url(" + "includes/images/menu_over_" + id + ".jpg)";
			G_currentMenu = id;
			window.parent.switchLoc(loc);
		}
	}
	
	function showAccount() {
		Collapsar.publish("/account/show", {});
	}
	
	function showAbout() {
		Collapsar.publish("/help/showabout", {});
	}
</script>
</head>
<body style="margin:0px;overflow-x:hidden;overflow-y:hidden;">
<div id="top_banner">
	<div style="float:left;padding:0px;margin:0px;"><img src="includes/nsfocus_2009/images/top_logo_nta.png" alt=""/></div>
<div id="banner_right">
<div>
<img src="includes/images/top_arrow.gif" align="middle" alt="帐号管理"/><a href="javascript:showAccount()">帐号管理</a><img src="includes/images/top_arrow.gif" align="middle" alt="绿盟科技" class="margin_left"/><a href="http://www.nsfocus.com" target="_blank">绿盟科技</a><img src="includes/images/top_arrow.gif" align="middle" alt="关于NTA" class="margin_left"/><a href="javascript:showAbout();">关于NTA</a></div>
</div>
</div>
<#global menu_index = 0>
<div id="topMenu">
	<div style="float:left"><img src="includes/images/menu_04.jpg" alt="menu"/></div>
	<@menu_item id="event" text="告警监控" selected=true/>
	<#if _LicPermit(_LicConfig.SNMP_TRAFPERF)>
	<@menu_item id="device" text="设备监控"/>
	</#if>
	<@menu_item id="traffic" text="流量分析"/>
	<#if _LicPermit(_LicConfig.ROUTE_ANALYSE)>
	<@menu_item id="route" text="路由分析"/>
	</#if>
	<@menu_item id="report" text="统计报表"/>
	<@menu_item id="log" text="日志管理"/>
	<#if _AclPrint('superuser', 'admin')>
	<@menu_item id="runtime" text="运行配置"/>
	</#if>
	<#if _AclPrint('superuser')>
	<@menu_item id="system" text="系统管理"/>
	</#if>
	<div id="cancel" style="float:right"><img src="includes/images/cancel.jpg" style="cursor:hand" onclick="top.document.location='auth/logout.action'" alt="注销"/></div>
	<div style="float:left"><img src="includes/images/menu_11.jpg" alt="menu"/></div>
</div>
</body>
</html>

<#macro menu_item id text selected=false>
<#assign menu_index=menu_index+1>
<div id="menu_${menu_index}" class="menu_${selected?string('selected','unselected')}" onclick="initMenu(${menu_index}, '${id}')">${text}</div>
</#macro>
